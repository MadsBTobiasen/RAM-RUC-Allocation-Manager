                @foreach (PromotionCommitteeTask pct in Model.Employee.PromotionCommittees)
                {
                    <tr>
                        <td></td>
                        <td>@pct.PeopleToBeAssessed</td>
                        <input type="hidden" name="gftId" value="@pct.Id" />
                        <input type="hidden" name="userId" value="@Model.Employee.Id" />
                        <td><input asp-page-handler="RemovePromotionCommittee" type="submit" value="Fjern" /></td>
                    </tr>
                }
            </tbody>
        }
    </table>
</form>

