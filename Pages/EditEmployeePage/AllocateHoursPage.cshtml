@model RAM___RUC_Allocation_Manager.Pages.EditEmployeePage.AllocateHoursPageModel
@{
}
<h1>Timeallokering for @Model.Employee.Name</h1>
<form method="post">

    <h3>Gruppeleder</h3>

    <div class="mb-3">
        <div class="custom-control custom-switch">
            <input asp-for="@Model.Employee.IsGroupLeader" type="checkbox" class="custom-control-input" id="customSwitch" name="customSwitch">
            <label class="custom-control-label" for="customSwitch">Er gruppeleder?</label>
        </div>

    </div>

    <input type="hidden" name="userId" value="@Model.Employee.Id" />
    <input asp-page-handler="GroupLeader" type="submit" value="Gem" class="btn btn-primary" />

</form>

<hr />

<form method="post">

    <h3>Opsparing</h3>

    <div class="form-group">
        <select asp-for="Employee.Savings" name="savings" class="form-control mb-1">
            <option value="@Employee.EmployeeSavings.ZeroPercent">0 procent opsparet</option>
            <option value="@Employee.EmployeeSavings.TwentyPercent">20 procent opsparet</option>
            <option value="@Employee.EmployeeSavings.FortyPercent">40 procent opsparet</option>
            <option value="@Employee.EmployeeSavings.SixtyPercent">60 procent opsparet</option>
            <option value="@Employee.EmployeeSavings.EightyPercent">80 procent opsparet</option>
            <option value="@Employee.EmployeeSavings.HundredPercent">100 procent opsparet</option>
        </select>
    </div>

    <input type="hidden" name="userId" value="@Model.Employee.Id" />
    <input asp-page-handler="Savings" type="submit" value="Gem" class="btn btn-primary" />

</form>

<hr />

@if (Model.Employee.Title != Employee.EmployeeTitle.AssistantProfessor)
{
<form method="post">

    <h3>Frikøb</h3>

    <div class="row">

        <div class="col-3 text-center"><h5>Start dato</h5></div>
        <div class="col-3 text-center"><h5>Slut dato</h5></div>
        <div class="col-3 text-center"><h5>Tid frikøbt</h5></div>
        <div class="col-3"></div>

        @foreach (Redemption redemption in Model.Employee.Redemptions)
        {

            <div class="col-3">
                <p class="">@redemption.StartDate</p>
            </div>
            <div class="col-3">
                <p class="">@redemption.EndDate</p>
            </div>
            <div class="col-3">
                <p class="">@Model.ConvertMinutesToHours(redemption.RedeemedMinutes)</p>
            </div>
            <div class="col-3">

                <input type="hidden" name="id" value="@redemption.Id" />
                <input type="hidden" name="userId" value="@Model.Employee.Id" />
                <input asp-page-handler="DeleteRedemption" type="submit" value="fjern frikøb" class="btn btn-primary float-right" />

            </div>

        }

    </div>

    <div class="justify-content-between">
        <hr class="w-75" />
    </div>

    <div class="form-group">

        <h3>Nyt frikøb</h3>

        <h5>Start dato:</h5>
        <input class="form-control mb-1" name="startDate" type="date" />

        <h5>Slut dato:</h5>
        <input class="form-control mb-1" name="endDate" type="date" /> Slut dato

        <h5>Frikøbte timer</h5>
        <input class="form-control mb-1" name="hours" pattern="[0-9]+" />

        <h5>Frikøbte minutter</h5>
        <input class="form-control mb-1" name="minutes" pattern="([1-5][0-9]|[1-9])" />

        <input type="hidden" name="userId" value="@Model.Employee.Id" />
        <input asp-page-handler="Redeem" type="submit" value="Tilføj frikøb" class="btn btn-primary" />

    </div>

</form>

<hr />

}

<form method="post">

    <h3>Gæld & Overskud</h3>

    <div class="form-group">

        <h5 class="mb-1">Nuværende gæld/overskud: <span class="font-weight-normal">@Model.ConvertMinutesToHours(Model.Employee.Balance)</span></h5>
        <h5 class="mb-1">Gæld/OverSkud for nyt semester: <span class="font-weight-normal">@Model.ConvertMinutesToHours(Model.Employee.CalculateNewMinuteBalance(Model.BaseSettings))</span></h5>
        <input type="hidden" name="userId" value="@Model.Employee.Id" />
        <input asp-page-handler="AutoBalance" type="submit" value="Gem" class="btn btn-primary" />

    </div>

    <div class="form-group">

        <h5 class="mb-1">Manuelt indstil timer</h5>
        <input class="mb-1" name="hours" pattern="[0-9]+" />

        <h5 class="mb-1">Manuelt indstil minutter</h5>
        <input class="mb-1" name="minutes" pattern="([1-5][0-9]|[1-9])" />

        <h5></h5>

        <input type="hidden" name="userId" value="@Model.Employee.Id" />
        <input asp-page-handler="ManualBalance" type="submit" value="Gem" class="btn btn-primary" />

    </div>    
        
</form>

<hr />

<form method="post">

    <h3>Underviser på</h3>

    <div class="row">

        <div class="col-3 text-center"><h5>Navn</h5></div>
        <div class="col-3 text-center"><h5>Type</h5></div>
        <div class="col-3"></div>

        @if (Model.Employee.EmployeeCourses.Any())
        {
            @foreach (Course course in Model.Employee.EmployeeCourses.Select(ep => ep.Course))
            {

                <div class="col-3">
                    <p class="">@course.Name</p>
                </div>
                <div class="col-3">
                    <p class="">@course.Type.ToString()</p>
                </div>
                <div class="col-3">

                    <input type="hidden" name="removeId" value="@course.Id" />
                    <input type="hidden" name="userId" value="@Model.Employee.Id" />
                    <input asp-page-handler="RemoveCourse" type="submit" value="Fjern kursus" class="btn btn-primary float-right" />

                </div>

            }
        }

    </div>

</form>

<hr />

<form method="post">

    <h3>Øvrige kurser</h3>

    <div class="row">

        <div class="col-3 text-center"><h5>Navn</h5></div>
        <div class="col-3 text-center"><h5>Type</h5></div>
        <div class="col-3 text-center"><h5>Lecture antal</h5></div>
        <div class="col-3"><h5>Uddel antal timer</h5></div>
        <div class="col-3"></div>

        @if (Model.Courses != null || Model.Courses.Any())
        {
            @foreach (Course course in Model.Courses.Where(c => !Model.Employee.EmployeeCourses.ToList().Select(ec => ec.CourseId).Contains(c.Id)).Select(c => c))
            {

                <div class="col-3">
                    <p class="">@course.Name</p>
                </div>
                <div class="col-3">
                    <p class="">@course.Type.ToString()</p>
                </div>
                <div class="col-3">
                    <p class="">@course.LectureAmount</p>
                </div>
                <div class="col-3">

                    <h5 class="mb-1">Uddel antal timer</h5>
                    <input type="hidden" name="addId" value="@course.Id" />
                    <input name="relativeLectureAmount" pattern="[0-9]+" size="1" />

                </div>
                <div class="col-3">

                    <input type="hidden" name="userId" value="@Model.Employee.Id" />
                    <input asp-page-handler="AddCourse" type="submit" value="Tilføj kursus" class="btn btn-primary" />

                </div>

            }
        }

    </div>

</form>

<hr />

